{
  "manifest_version": 3,
  "name": "yt-toc",
  "description": "test",
  "version": "0.1",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "permissions": [
    "sidePanel",
    "scripting",
    "contextMenus",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}